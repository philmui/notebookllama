    "version": "0.2.0",
    "configurations": [
        {
            "name": "uv extract agent",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/tools/create_llama_extract_agent.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "uv index agent",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/tools/create_llama_cloud_index.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}